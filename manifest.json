{
  "name": "Swiggy Spending Calculator",
  "version": "1.0.0",
  "description": "Extension to calculate total spending on swiggy till date",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://www.swiggy.com/*"],
      "js": ["content.js"]
    }
  ]
}